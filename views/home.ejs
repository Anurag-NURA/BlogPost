<%- include("partials/header"); %>
<div class="home">
  <h1>Home</h1>
  <p><%= startingContent %></p>
</div>

<% homePosts.forEach(element => { %>
<h1><%= element.title %></h1>
<p>
  <%= element.content.substring(0, 100) + "..." %><a
    href="http://localhost:8000/posts/<%= element._id %> "
    >Read more</a
  >
</p>

<% });%> <%- include("partials/footer"); %>
